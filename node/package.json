{
  "name": "@sunntics/appian",
  "version": "3.7.23",
  "description": "A very opinionated, convenience wrapper around Express and other packages for API development.",
  "scope": "@sunntics",
  "author": {
    "name": "Juan Pedro Perez Alcantara",
    "email": "jp.perez.alcantara@gmail.com",
    "url": "http://me.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "appian",
    "API"
  ],
  "license": "UNLICENSED",
  "homepage": "https://github.com/owner/project#readme",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/malkab/code-samples.git"
  },
  "private": false,
  "scripts": {
    "build-prod": "yarn clean ; NODE_ENV=production yarn build",
    "build": "yarn clean ; npm run service:build-lib",
    "clean": "rm -Rf build ; rm -Rf dist",
    "mocha": "npm-run-all --parallel service:watch:mocha:server service:build-tests",
    "publish-prod": "yarn publish --patch --no-git-tag-version",
    "quick-test": "npm-run-all --parallel service:watch:quick-test:server service:build",
    "service:build-lib": "webpack --config webpack-production.config.js --progress",
    "service:build-tests": "webpack --progress --config webpack-tests.config.js",
    "service:build": "webpack --progress",
    "service:watch:main:server": "nodemon --inspect=0.0.0.0:9229 \"./build/main.js\" --watch \"./build/main.js\"",
    "service:watch:mocha:server": "nodemon --ext js --watch ./build/mocha.js --exec 'mocha --timeout 20000 ./build/mocha.js || true'",
    "service:watch:quick-test:server": "nodemon --inspect=0.0.0.0:9329 \"./build/quicktest.js\" --watch \"./build/quicktest.js\"",
    "start": "npm-run-all --parallel service:watch:main:server service:build"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.5",
    "@types/chai": "4.3.20",
    "@types/cookie-parser": "1.4.8",
    "@types/cors": "2.8.18",
    "@types/express": "4.17.22",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash": "4.17.16",
    "@types/mocha": "9.0.0",
    "@types/morgan": "1.9.9",
    "@types/multer": "1.4.12",
    "@types/node": "22.15.19",
    "axios": "1.9.0",
    "chai": "4.5.0",
    "clean-webpack-plugin": "3.0.0",
    "eslint": "7.32.0",
    "mocha": "9.2.2",
    "nodemon": "2.0.22",
    "npm-run-all": "4.1.5",
    "terser": "5.39.2",
    "terser-webpack-plugin": "5.3.14",
    "ts-loader": "9.5.2",
    "typescript": "5.7.2",
    "webpack": "5.99.8",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "@sunntics/node-logger": "1.1.12",
    "@sunntics/node-utils": "1.6.51",
    "@sunntics/rxpg": "5.0.8",
    "@sunntics/rxredis": "2.3.17",
    "@sunntics/ts-utils": "8.0.11",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "express": "4.21.2",
    "http-status-codes": "2.3.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.2",
    "winston": "3.10.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
